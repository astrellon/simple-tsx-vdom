{
  "name": "simple-tsx-vdom",
  "version": "0.0.3",
  "description": "A simple virtual DOM with TSX support, not made for performance but for learning",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.js",
  "repository": "github:astrellon/simple-tsx-vdom",
  "keywords": [
      "vdom",
      "framework",
      "learning",
      "ui",
      "user interface",
      "virtual dom",
      "dom diff"
  ],
  "files": [
      "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/jsx.d.ts dist/jsx.d.ts && terser -c -m toplevel -o dist/index.min.js --source-map filename=dist/input.js.map dist/index.js",
    "test": "jest --verbose",
    "coverage": "jest --coverage --coverageDirectory='coverage'"
  },
  "npmName": "simple-tsx-vdom",
  "author": "Alan Lawrey",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "core-js": "^3.8.3",
    "jest": "^26.6.3",
    "terser": "^5.6.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  }
}
